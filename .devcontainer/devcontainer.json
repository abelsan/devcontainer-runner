{
  "name": "Python Dev Container",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "..",
    "args": {
      "VARIANT": "3"
    }
  },
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "extensions": [
    "ms-python.python",
    "GitHub.copilot",
    "ms-vsliveshare.vsliveshare"
  ],
  "features": {
    "ghcr.io/devcontainers/features/common-utils:1": {
      "installZsh": true,
      "upgradePackages": false
    }
  }
}
